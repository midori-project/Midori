{
  "name": "orchestrator", 
  "description": "วางแผนและประสานงาน agents ตาม Command ที่ได้รับ",
  "model": "gpt-5",
  "temperature": 0.1,
  "produces": [
    "OrchestratorPlan",
    "Task",
    "Edge",
    "CommandEvent"
  ],
  "consumes": [
    "Command"
  ],
  "schemas": {
    "command": "../schemas/command.schema.json",
    "plan": "../schemas/plan.schema.json",
    "task": "../schemas/task.schema.json"
  },
  "policies": {
    "qualityGates": "../policies/quality-gates.json",
    "escalation": "../policies/escalation.json",
    "interaction": "../policies/interaction.json"
  },
  "systemPrompt": "../../prompts/orchestrator.system.md",
  "capabilities": [
    "dag_planning",
    "agent_coordination", 
    "dependency_resolution",
    "parallel_execution",
    "workflow_optimization"
  ],
  "triggers": [
    "command_received",
    "agent_task_completion",
    "workflow_failure"
  ],
  "features": {
    "maxParallelTasks": 5,
    "retryAttempts": 3,
    "timeoutMinutes": 30,
    "dependencyTracking": true
  }
}
