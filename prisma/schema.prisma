generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Agent {
  id           String         @id
  projectId    String?
  userId       String?
  name         String
  description  String?
  kind         AgentKind      @default(chat)
  model        String
  systemPrompt String?
  tools        Json?
  config       Json?
  temperature  Float?         @default(0.7)
  topP         Float?         @default(1)
  isActive     Boolean        @default(true)
  createdAt    DateTime       @default(now())
  updatedAt    DateTime
  Project      Project?       @relation(fields: [projectId], references: [id], onDelete: Cascade)
  User         User?          @relation(fields: [userId], references: [id])
  ChatRun      ChatRun[]
  Command      Command[]
  Conversation Conversation[]

  @@index([projectId])
  @@index([userId])
}

model Artifact {
  id                String          @id
  projectId         String
  snapshotId        String?
  specBundleVersion Int?
  createdAt         DateTime        @default(now())
  buildFramework    String
  buildEngine       String?
  buildNode         String
  buildCli          String?
  buildHash         String
  buildRunner       String?
  buildDurationMs   Int?
  storageProvider   String?
  storageBucket     String?
  storagePrefix     String?
  storageRegion     String?
  gateStatus        GateStatus?
  failingGates      Json?
  coverage          Decimal?        @db.Decimal(3, 2)
  testSummary       String?
  previewEnabled    Boolean         @default(false)
  previewTtl        Int?
  previewToken      String?
  previewUrl        String?
  deployProvider    DeployProvider?
  deployState       DeployState?
  deployUrl         String?
  deployCacheKey    String?
  deployMeta        Json?
  manifestHash      String?
  signature         String?
  signedBy          String?
  Project           Project         @relation(fields: [projectId], references: [id], onDelete: Cascade)
  Snapshot          Snapshot?       @relation(fields: [snapshotId], references: [id])
  ArtifactFile      ArtifactFile[]
  ArtifactLink      ArtifactLink[]

  @@index([deployState, createdAt])
  @@index([gateStatus, createdAt])
  @@index([projectId, createdAt])
  @@index([snapshotId])
}

model ArtifactFile {
  id                String    @id
  artifactId        String
  path              String
  mimeType          String
  hash              String
  size              Int?
  cacheTtl          Int?
  cacheImmutable    Boolean?
  cacheHeaders      Json?
  content           String?
  externalUrl       String?
  externalSigned    Boolean?
  externalExpiresAt DateTime?
  Artifact          Artifact  @relation(fields: [artifactId], references: [id], onDelete: Cascade)

  @@unique([artifactId, path])
  @@index([hash])
}

model ArtifactLink {
  id         String   @id
  artifactId String
  kind       String
  url        String?
  filePath   String?
  meta       Json?
  createdAt  DateTime @default(now())
  Artifact   Artifact @relation(fields: [artifactId], references: [id], onDelete: Cascade)

  @@index([artifactId, kind])
}

model AuthCredential {
  id                    String                  @id
  userId                String
  type                  String
  identifier            String
  secret                String?
  metadata              Json?
  isVerified            Boolean                 @default(false)
  expiresAt             DateTime?
  createdAt             DateTime                @default(now())
  updatedAt             DateTime
  User                  User                    @relation(fields: [userId], references: [id], onDelete: Cascade)
  AuthEmailVerification AuthEmailVerification[]
  AuthPasswordReset     AuthPasswordReset[]

  @@unique([userId, type, identifier])
  @@index([identifier])
  @@index([userId])
}

model AuthEmailVerification {
  id             String          @id
  userId         String
  credentialId   String?
  tokenHash      String
  expiresAt      DateTime
  usedAt         DateTime?
  createdAt      DateTime        @default(now())
  AuthCredential AuthCredential? @relation(fields: [credentialId], references: [id])
  User           User            @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@index([expiresAt])
  @@index([userId])
}

model AuthPasswordReset {
  id             String          @id
  userId         String
  credentialId   String?
  tokenHash      String
  expiresAt      DateTime
  usedAt         DateTime?
  createdAt      DateTime        @default(now())
  AuthCredential AuthCredential? @relation(fields: [credentialId], references: [id])
  User           User            @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@index([expiresAt])
  @@index([userId])
}

model Category {
  id              String            @id
  key             String            @unique
  label           String
  meta            Json?
  createdAt       DateTime          @default(now())
  ProjectCategory ProjectCategory[]
}

model ChatRun {
  id             String       @id
  conversationId String
  userId         String?
  projectId      String?
  agentId        String?
  model          String
  tokensInput    Int          @default(0)
  tokensOutput   Int          @default(0)
  costUsd        Decimal      @default(0)
  latencyMs      Int?
  status         String?
  error          String?
  rawRequest     Json?
  rawResponse    Json?
  createdAt      DateTime     @default(now())
  updatedAt      DateTime
  Agent          Agent?       @relation(fields: [agentId], references: [id])
  Conversation   Conversation @relation(fields: [conversationId], references: [id], onDelete: Cascade)
  Project        Project?     @relation(fields: [projectId], references: [id])
  User           User?        @relation(fields: [userId], references: [id])
  Message        Message[]
  PatchSet       PatchSet[]

  @@index([agentId])
  @@index([conversationId])
  @@index([projectId])
  @@index([userId])
}

model Command {
  id               String             @id
  projectId        String?
  userId           String?
  conversationId   String?
  sourceAgentId    String?
  target           String?
  commandType      String
  commandJson      Json
  status           CommandStatus      @default(queued)
  error            String?
  createdAt        DateTime           @default(now())
  startedAt        DateTime?
  finishedAt       DateTime?
  Conversation     Conversation?      @relation(fields: [conversationId], references: [id])
  Project          Project?           @relation(fields: [projectId], references: [id])
  Agent            Agent?             @relation(fields: [sourceAgentId], references: [id])
  User             User?              @relation(fields: [userId], references: [id])
  CommandEvent     CommandEvent[]
  OrchestratorPlan OrchestratorPlan[]

  @@index([conversationId])
  @@index([projectId])
  @@index([userId])
}

model CommandEvent {
  id        String   @id
  commandId String
  kind      String
  data      Json?
  createdAt DateTime @default(now())
  Command   Command  @relation(fields: [commandId], references: [id], onDelete: Cascade)

  @@index([commandId])
}

model Conversation {
  id         String     @id
  projectId  String?
  userId     String?
  agentId    String?
  title      String?
  visibility Visibility @default(private)
  context    Json?
  createdAt  DateTime   @default(now())
  updatedAt  DateTime
  archivedAt DateTime?
  ChatRun    ChatRun[]
  Command    Command[]
  Agent      Agent?     @relation(fields: [agentId], references: [id])
  Project    Project?   @relation(fields: [projectId], references: [id])
  User       User?      @relation(fields: [userId], references: [id])
  Message    Message[]

  @@index([agentId])
  @@index([projectId])
  @@index([userId])
}

model CopyBlock {
  id          String   @id
  projectId   String
  key         String
  locale      String   @default("th")
  content     String?
  contentJson Json?
  version     Int      @default(1)
  createdAt   DateTime @default(now())
  updatedAt   DateTime
  Project     Project  @relation(fields: [projectId], references: [id], onDelete: Cascade)

  @@unique([projectId, key, locale, version])
  @@index([projectId])
}

model Deployment {
  id        String         @id
  projectId String
  provider  DeployProvider
  state     DeployState    @default(queued)
  url       String?
  meta      Json?
  createdAt DateTime       @default(now())
  Project   Project        @relation(fields: [projectId], references: [id], onDelete: Cascade)

  @@index([projectId])
}

model File {
  id                                  String              @id
  projectId                           String
  path                                String
  type                                FileType            @default(code)
  isBinary                            Boolean             @default(false)
  content                             String?
  blob                                Bytes?
  createdAt                           DateTime            @default(now())
  updatedAt                           DateTime
  Project_File_projectIdToProject     Project             @relation("File_projectIdToProject", fields: [projectId], references: [id], onDelete: Cascade)
  ImageAsset                          ImageAsset[]
  MessageAttachment                   MessageAttachment[]
  Project_Project_previewFileIdToFile Project?            @relation("Project_previewFileIdToFile")
  TestArtifact                        TestArtifact[]

  @@unique([projectId, path])
  @@index([projectId])
}

model Generation {
  id             String           @id
  projectId      String
  userId         String
  prompt         Json?
  promptJson     Json?
  options        Json             @default("{}")
  model          String
  tokensInput    Int              @default(0)
  tokensOutput   Int              @default(0)
  costUsd        Decimal          @default(0)
  createdAt      DateTime         @default(now())
  Project        Project          @relation(fields: [projectId], references: [id], onDelete: Cascade)
  User           User             @relation(fields: [userId], references: [id], onDelete: Cascade)
  GenerationFile GenerationFile[]
  Snapshot       Snapshot[]

  @@index([projectId])
  @@index([userId])
}

model GenerationFile {
  id           String               @id
  generationId String
  filePath     String
  fileContent  String?
  changeType   GenerationChangeType @default(create)
  Generation   Generation           @relation(fields: [generationId], references: [id], onDelete: Cascade)

  @@index([generationId])
}

model ImageAsset {
  id         String      @id
  projectId  String
  briefId    String?
  fileId     String?
  provider   String?
  meta       Json?
  createdAt  DateTime    @default(now())
  ImageBrief ImageBrief? @relation(fields: [briefId], references: [id])
  File       File?       @relation(fields: [fileId], references: [id])
  Project    Project     @relation(fields: [projectId], references: [id], onDelete: Cascade)

  @@index([briefId])
  @@index([fileId])
  @@index([projectId])
}

model ImageBrief {
  id              String       @id
  projectId       String
  sourceMessageId String?
  prompt          String?
  promptJson      Json?
  styleHints      Json?
  createdAt       DateTime     @default(now())
  ImageAsset      ImageAsset[]
  Project         Project      @relation(fields: [projectId], references: [id], onDelete: Cascade)

  @@index([projectId])
}

model LoginAttempt {
  id         String        @id
  userId     String?
  loginEmail String?
  provider   LoginProvider
  ip         String?
  success    Boolean
  createdAt  DateTime      @default(now())
  User       User?         @relation(fields: [userId], references: [id])

  @@index([createdAt])
  @@index([userId])
}

model Message {
  id                String              @id
  conversationId    String
  userId            String?
  role              ChatRole
  content           String?
  contentJson       Json?
  runId             String?
  toolName          String?
  toolInput         Json?
  toolOutput        Json?
  messageIndex      Int
  createdAt         DateTime            @default(now())
  Conversation      Conversation        @relation(fields: [conversationId], references: [id], onDelete: Cascade)
  ChatRun           ChatRun?            @relation(fields: [runId], references: [id])
  User              User?               @relation(fields: [userId], references: [id])
  MessageAttachment MessageAttachment[]

  @@index([conversationId, messageIndex])
  @@index([runId])
  @@index([userId])
}

model MessageAttachment {
  id        String   @id
  messageId String
  fileId    String?
  url       String?
  kind      String?
  meta      Json?
  createdAt DateTime @default(now())
  File      File?    @relation(fields: [fileId], references: [id])
  Message   Message  @relation(fields: [messageId], references: [id], onDelete: Cascade)

  @@index([fileId])
  @@index([messageId])
}

model OAuthConnection {
  id                    String        @id
  userId                String
  provider              OAuthProvider
  providerAccountId     String
  accessTokenEncrypted  String?
  refreshTokenEncrypted String?
  scope                 String?
  expiresAt             DateTime?
  createdAt             DateTime      @default(now())
  User                  User          @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@unique([provider, providerAccountId])
  @@index([userId])
}

model OrchestratorEdge {
  id               String           @id
  planId           String
  fromTaskId       String
  toTaskId         String
  OrchestratorPlan OrchestratorPlan @relation(fields: [planId], references: [id], onDelete: Cascade)

  @@index([planId])
}

model OrchestratorPlan {
  id               String             @id
  projectId        String
  commandId        String?
  title            String?
  decision         Json?
  createdAt        DateTime           @default(now())
  OrchestratorEdge OrchestratorEdge[]
  Command          Command?           @relation(fields: [commandId], references: [id])
  Project          Project            @relation(fields: [projectId], references: [id], onDelete: Cascade)
  OrchestratorTask OrchestratorTask[]

  @@index([commandId])
  @@index([projectId])
}

model OrchestratorTask {
  id               String           @id
  planId           String
  taskKey          String
  to               String
  action           String
  payload          Json?
  status           TaskStatus       @default(pending)
  error            String?
  startedAt        DateTime?
  finishedAt       DateTime?
  createdAt        DateTime         @default(now())
  OrchestratorPlan OrchestratorPlan @relation(fields: [planId], references: [id], onDelete: Cascade)

  @@unique([planId, taskKey])
  @@index([planId])
}

model Patch {
  id         String               @id
  patchSetId String
  filePath   String
  changeType GenerationChangeType @default(update)
  hunks      Json?
  createdAt  DateTime             @default(now())
  PatchSet   PatchSet             @relation(fields: [patchSetId], references: [id], onDelete: Cascade)

  @@unique([patchSetId, filePath])
  @@index([filePath])
  @@index([patchSetId])
}

model PatchSet {
  id                                         String    @id
  projectId                                  String
  fromSnapshotId                             String?
  toSnapshotId                               String?
  createdByRunId                             String?
  meta                                       Json?
  createdAt                                  DateTime  @default(now())
  Patch                                      Patch[]
  ChatRun                                    ChatRun?  @relation(fields: [createdByRunId], references: [id])
  Snapshot_PatchSet_fromSnapshotIdToSnapshot Snapshot? @relation("PatchSet_fromSnapshotIdToSnapshot", fields: [fromSnapshotId], references: [id])
  Project                                    Project   @relation(fields: [projectId], references: [id], onDelete: Cascade)
  Snapshot_PatchSet_toSnapshotIdToSnapshot   Snapshot? @relation("PatchSet_toSnapshotIdToSnapshot", fields: [toSnapshotId], references: [id])

  @@index([createdByRunId])
  @@index([projectId])
}

model PreviewSession {
  id         String    @id
  projectId  String
  snapshotId String?
  url        String?
  state      String?
  meta       Json?
  createdAt  DateTime  @default(now())
  Project    Project   @relation(fields: [projectId], references: [id], onDelete: Cascade)
  Snapshot   Snapshot? @relation(fields: [snapshotId], references: [id])

  @@index([projectId])
  @@index([snapshotId])
}

model Project {
  id                               String             @id
  ownerId                          String
  name                             String
  description                      String?
  visibility                       Visibility         @default(private)
  options                          Json               @default("{}")
  likeCount                        Int                @default(0)
  createdAt                        DateTime           @default(now())
  updatedAt                        DateTime
  deletedAt                        DateTime?
  previewFileId                    String?            @unique
  tenantId                         String?
  Agent                            Agent[]
  Artifact                         Artifact[]
  ChatRun                          ChatRun[]
  Command                          Command[]
  Conversation                     Conversation[]
  CopyBlock                        CopyBlock[]
  Deployment                       Deployment[]
  File_File_projectIdToProject     File[]             @relation("File_projectIdToProject")
  Generation                       Generation[]
  ImageAsset                       ImageAsset[]
  ImageBrief                       ImageBrief[]
  OrchestratorPlan                 OrchestratorPlan[]
  PatchSet                         PatchSet[]
  PreviewSession                   PreviewSession[]
  User                             User               @relation(fields: [ownerId], references: [id], onDelete: Cascade)
  File_Project_previewFileIdToFile File?              @relation("Project_previewFileIdToFile", fields: [previewFileId], references: [id])
  Tenant                           Tenant?            @relation(fields: [tenantId], references: [id])
  ProjectCategory                  ProjectCategory[]
  ProjectContext                   ProjectContext?
  ProjectLike                      ProjectLike[]
  Snapshot                         Snapshot[]
  SpecBundle                       SpecBundle[]
  TestRun                          TestRun[]
  TestSuite                        TestSuite[]

  @@index([ownerId])
  @@index([tenantId])
}

model ProjectCategory {
  id         String   @id
  projectId  String
  categoryId String
  order      Int?
  createdAt  DateTime @default(now())
  Category   Category @relation(fields: [categoryId], references: [id], onDelete: Cascade)
  Project    Project  @relation(fields: [projectId], references: [id], onDelete: Cascade)

  @@unique([projectId, categoryId])
  @@index([categoryId])
  @@index([projectId])
}

model ProjectContext {
  id                  String        @id
  projectId           String        @unique
  specBundleId        String
  projectType         ProjectType
  status              ProjectStatus @default(created)
  components          Json
  pages               Json
  styling             Json
  conversationHistory Json
  userPreferences     Json
  lastModified        DateTime      @default(now())
  createdAt           DateTime      @default(now())
  updatedAt           DateTime
  Project             Project       @relation(fields: [projectId], references: [id], onDelete: Cascade)

  @@index([projectId])
  @@index([specBundleId])
}

model ProjectLike {
  id        String   @id
  projectId String
  userId    String
  createdAt DateTime @default(now())
  Project   Project  @relation(fields: [projectId], references: [id], onDelete: Cascade)
  User      User     @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@unique([projectId, userId])
  @@index([projectId])
  @@index([userId])
}

model Session {
  id               String    @id
  userId           String
  sessionTokenHash String    @unique
  ip               String?
  userAgent        String?
  createdAt        DateTime  @default(now())
  expiresAt        DateTime
  lastActiveAt     DateTime? @default(now())
  terminatedAt     DateTime?
  User             User      @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@index([expiresAt])
  @@index([userId])
}

model Snapshot {
  id                                         String           @id
  projectId                                  String
  label                                      String?
  files                                      Json
  fromGenerationId                           String?
  createdAt                                  DateTime         @default(now())
  Artifact                                   Artifact[]
  PatchSet_PatchSet_fromSnapshotIdToSnapshot PatchSet[]       @relation("PatchSet_fromSnapshotIdToSnapshot")
  PatchSet_PatchSet_toSnapshotIdToSnapshot   PatchSet[]       @relation("PatchSet_toSnapshotIdToSnapshot")
  PreviewSession                             PreviewSession[]
  Generation                                 Generation?      @relation(fields: [fromGenerationId], references: [id])
  Project                                    Project          @relation(fields: [projectId], references: [id], onDelete: Cascade)

  @@index([projectId])
}

model SpecBundle {
  id          String        @id
  projectId   String
  label       String?
  version     Int           @default(1)
  data        Json
  createdAt   DateTime      @default(now())
  updatedAt   DateTime
  Project     Project       @relation(fields: [projectId], references: [id], onDelete: Cascade)
  SpecVersion SpecVersion[]

  @@index([projectId])
}

model SpecVersion {
  id         String     @id
  bundleId   String
  version    Int
  diff       Json?
  snapshot   Json
  createdAt  DateTime   @default(now())
  SpecBundle SpecBundle @relation(fields: [bundleId], references: [id], onDelete: Cascade)

  @@unique([bundleId, version])
  @@index([bundleId])
}

model Tag {
  id                String              @id
  key               String              @unique
  label             String
  createdAt         DateTime            @default(now())
  UiTemplateMetaTag UiTemplateMetaTag[]
}

model TemplateSourceFile {
  id                String            @id
  versionId         String
  path              String
  type              String?
  content           String
  encoding          String
  contentType       String?
  size              Int
  sha256            String
  createdAt         DateTime          @default(now())
  UiTemplateVersion UiTemplateVersion @relation(fields: [versionId], references: [id], onDelete: Cascade)

  @@unique([versionId, path])
  @@index([sha256])
  @@index([versionId])
}

model TemplateSourceSummary {
  id                String            @id
  versionId         String            @unique
  filesCount        Int
  sizeBytes         Int
  checksum          String
  createdAt         DateTime          @default(now())
  UiTemplateVersion UiTemplateVersion @relation(fields: [versionId], references: [id], onDelete: Cascade)
}

model Tenant {
  id        String    @id
  key       String    @unique
  name      String?
  createdAt DateTime  @default(now())
  Project   Project[]
}

model TestArtifact {
  id        String   @id
  runId     String
  kind      String
  url       String?
  fileId    String?
  meta      Json?
  createdAt DateTime @default(now())
  File      File?    @relation(fields: [fileId], references: [id])
  TestRun   TestRun  @relation(fields: [runId], references: [id], onDelete: Cascade)

  @@index([fileId])
  @@index([runId])
}

model TestCase {
  id         String    @id
  suiteId    String
  name       String
  definition Json?
  createdAt  DateTime  @default(now())
  TestSuite  TestSuite @relation(fields: [suiteId], references: [id], onDelete: Cascade)
  TestRun    TestRun[]

  @@index([suiteId])
}

model TestRun {
  id                String         @id
  projectId         String
  suiteId           String?
  caseId            String?
  triggeredByTaskId String?
  status            TestStatus     @default(queued)
  stats             Json?
  startedAt         DateTime?
  finishedAt        DateTime?
  createdAt         DateTime       @default(now())
  TestArtifact      TestArtifact[]
  TestCase          TestCase?      @relation(fields: [caseId], references: [id])
  Project           Project        @relation(fields: [projectId], references: [id], onDelete: Cascade)
  TestSuite         TestSuite?     @relation(fields: [suiteId], references: [id])

  @@index([caseId])
  @@index([projectId])
  @@index([suiteId])
}

model TestSuite {
  id        String     @id
  projectId String
  name      String
  kind      String
  meta      Json?
  createdAt DateTime   @default(now())
  TestCase  TestCase[]
  TestRun   TestRun[]
  Project   Project    @relation(fields: [projectId], references: [id], onDelete: Cascade)

  @@index([projectId])
}

model UiTemplate {
  id                String              @id
  key               String              @unique
  label             String
  category          String?
  createdAt         DateTime            @default(now())
  UiTemplateMeta    UiTemplateMeta?
  UiTemplateVersion UiTemplateVersion[]
}

model UiTemplateMeta {
  id                String              @id
  templateId        String              @unique
  description       String?
  engine            String?
  status            TemplateStatus?     @default(draft)
  versioningPolicy  String?
  previewScreenshot String?
  compat            Json?
  postProcess       Json?
  author            String?
  createdAt         DateTime            @default(now())
  updatedAt         DateTime
  UiTemplate        UiTemplate          @relation(fields: [templateId], references: [id], onDelete: Cascade)
  UiTemplateMetaTag UiTemplateMetaTag[]

  @@index([createdAt])
  @@index([engine])
  @@index([status])
}

model UiTemplateMetaTag {
  id             String         @id
  metaId         String
  tagId          String
  createdAt      DateTime       @default(now())
  UiTemplateMeta UiTemplateMeta @relation(fields: [metaId], references: [id], onDelete: Cascade)
  Tag            Tag            @relation(fields: [tagId], references: [id], onDelete: Cascade)

  @@unique([metaId, tagId])
  @@index([metaId])
  @@index([tagId])
}

model UiTemplateVersion {
  id                    String                 @id
  templateId            String
  version               Int
  files                 Json?
  slots                 Json?
  constraints           Json?
  createdAt             DateTime               @default(now())
  checksum              String?
  compat                Json?
  deprecatedAt          DateTime?
  publishedAt           DateTime?
  semver                String?
  sizeBytes             Int?
  status                TemplateStatus         @default(published)
  TemplateSourceFile    TemplateSourceFile[]
  TemplateSourceSummary TemplateSourceSummary?
  UiTemplate            UiTemplate             @relation(fields: [templateId], references: [id], onDelete: Cascade)

  @@unique([templateId, version])
  @@index([createdAt])
  @@index([status])
  @@index([templateId])
}

model User {
  id                    String                  @id
  email                 String?                 @unique
  displayName           String?
  avatarUrl             String?
  locale                String?                 @default("th")
  isActive              Boolean                 @default(true)
  createdAt             DateTime                @default(now())
  updatedAt             DateTime
  lastLoginAt           DateTime?
  Agent                 Agent[]
  AuthCredential        AuthCredential[]
  AuthEmailVerification AuthEmailVerification[]
  AuthPasswordReset     AuthPasswordReset[]
  ChatRun               ChatRun[]
  Command               Command[]
  Conversation          Conversation[]
  Generation            Generation[]
  LoginAttempt          LoginAttempt[]
  Message               Message[]
  OAuthConnection       OAuthConnection[]
  Project               Project[]
  ProjectLike           ProjectLike[]
  Session               Session[]

  @@index([email])
}

enum AgentKind {
  chat
  orchestrator
  blueprint
  copywriter
  visual
  editor
  squasher
  tester
  debugger
}

enum ChatRole {
  user
  assistant
  system
  tool
}

enum CommandStatus {
  queued
  running
  completed
  failed
  cancelled
}

